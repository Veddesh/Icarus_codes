#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5574cbe50c80 .scope module, "Testbench" "Testbench" 2 4;
 .timescale -9 -12;
v0x5574cbe63f90_0 .var "Clock", 0 0;
v0x5574cbe64060_0 .net "Count", 3 0, v0x5574cbe63d90_0;  1 drivers
v0x5574cbe64130_0 .var "Reset", 0 0;
S_0x5574cbe50e10 .scope module, "I1" "Mycounter" 2 10, 3 4 0, S_0x5574cbe50c80;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "CLK";
    .port_info 1 /INPUT 1 "RST";
    .port_info 2 /OUTPUT 4 "OUT";
v0x5574cbe17490_0 .net "CLK", 0 0, v0x5574cbe63f90_0;  1 drivers
v0x5574cbe63d90_0 .var "OUT", 3 0;
v0x5574cbe63e70_0 .net "RST", 0 0, v0x5574cbe64130_0;  1 drivers
E_0x5574cbe17b30 .event posedge, v0x5574cbe17490_0;
    .scope S_0x5574cbe50e10;
T_0 ;
    %wait E_0x5574cbe17b30;
    %load/vec4 v0x5574cbe63e70_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5574cbe63d90_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5574cbe63d90_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5574cbe63d90_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5574cbe50c80;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5574cbe63f90_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5574cbe50c80;
T_2 ;
    %vpi_call 2 14 "$display", "Starting simulation..." {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5574cbe64130_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5574cbe64130_0, 0, 1;
    %delay 2000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5574cbe64130_0, 0, 1;
    %delay 400000, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5574cbe50c80;
T_3 ;
    %delay 50000, 0;
    %load/vec4 v0x5574cbe63f90_0;
    %inv;
    %store/vec4 v0x5574cbe63f90_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5574cbe50c80;
T_4 ;
    %vpi_call 2 26 "$dumpfile", "count.vcd" {0 0 0};
    %vpi_call 2 27 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5574cbe50c80 {0 0 0};
    %vpi_call 2 28 "$monitor", "%d, %b, %b, %d", $time, v0x5574cbe63f90_0, v0x5574cbe64130_0, v0x5574cbe64060_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Testbench.v";
    "Mycounter.v";
